// gluster的配置文件
{
    "SavePath" : "", // 数据保存路径(请保证运行gcluster的用户有权限写入)
    "MinNode"  : 1,  // (可选)组成gluster的最小节点数量，默认为1，常见集群一般为3
    "LogPath"  : "", // (可选)保存日志文件的文件夹目录路径，如果为空，表示不写入文件，直接输出到终端
    "Monitor"  : "", // (可选)监控节点IP或域名地址
    "Peers"    : [], // (可选)初始化节点列表，包含自定义的所需添加的服务器IP或者域名列表
    "DataMap"  : {}, // (可选)初始化自定义的k-v数据
    "Service"  : {}  // (可选)初始化服务配置
}